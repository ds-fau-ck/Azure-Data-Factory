{
	"name": "SalesDataTransformationFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceSalesData",
						"type": "DatasetReference"
					},
					"name": "FaueDeSourceSales"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable",
						"type": "DatasetReference"
					},
					"name": "FauDeSinkSnowflake"
				}
			],
			"transformations": [
				{
					"name": "FauDeFilter"
				},
				{
					"name": "FauDeAggregate"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ORDERNUMBER as short,",
				"          QUANTITYORDERED as short,",
				"          PRICEEACH as double,",
				"          ORDERLINENUMBER as short,",
				"          SALES as double,",
				"          ORDERDATE as timestamp,",
				"          STATUS as string,",
				"          QTR_ID as short,",
				"          MONTH_ID as short,",
				"          YEAR_ID as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> FaueDeSourceSales",
				"FaueDeSourceSales filter(SALES>=2000) ~> FauDeFilter",
				"FauDeFilter aggregate(groupBy(MONTH_ID,",
				"          YEAR_ID),",
				"     Total_Sales = sum(SALES)) ~> FauDeAggregate",
				"FauDeAggregate sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          YEAR_ID as string,",
				"          MONTH_ID as string,",
				"          TOTALSALES as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     stageInsert: true) ~> FauDeSinkSnowflake"
			]
		}
	}
}